<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="fill_parent"
    android:layout_height="300dp"
    tools:context="com.example.kimteaho.copy.LoginActivity">

    // 타이틀바 형태

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="52dp"
        android:id="@+id/titlebar_sign"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:padding="0dp"
        android:src="@drawable/titlebar"
        />

    // 뒤로가기 버튼

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/btnBackSign"
        android:src="@drawable/btnback_unclicked"
        android:background="@null"/>

    // 타이틀바 이름
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="수령확인"
        android:id="@+id/txt_title_name"
        android:textSize="18sp"
        android:textColor="#ffffff"
        android:textAlignment="center"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="110dp"
        android:layout_centerHorizontal="true"
        />

    //수령 배경

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:id="@+id/bg_receipt"
        android:background="@drawable/bg_list"
        android:layout_below="@+id/titlebar_sign"
        android:layout_marginTop="5dp"
        android:layout_alignStart="@+id/btnBackSign"
        android:layout_marginRight="10dp" />

    //과목명

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/subject_name1"
        android:text="경제경영수학"
        android:textSize="12sp"
        android:textColor="#FF863E"
        android:layout_alignTop="@+id/bg_receipt"
        android:layout_alignStart="@+id/bg_receipt"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="12dp"/>

    //파일명

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/file_name1"
        android:text="Ch.8 Linear Algebra.hwp"
        android:textSize="14sp"
        android:textColor="#A09F9F"
        android:layout_below="@+id/subject_name1"
        android:layout_marginTop="10dp"
        android:layout_alignStart="@+id/subject_name1" />

    //인쇄정보

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/print_info1"
        android:text="236-1-1-4"
        android:textSize="24sp"
        android:textColor="#000000"
        android:layout_below="@+id/file_name1"
        android:layout_marginTop="48dp"
        android:layout_alignStart="@+id/subject_name1" />

    //시간아이콘

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/duetime1"
        android:src="@drawable/list_duetime"
        android:layout_below="@+id/print_info1"
        android:layout_marginTop="15dp"
        android:layout_alignStart="@+id/subject_name1" />

    //시간 텍스트

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txt_due_time1"
        android:text="2015/08/18/12:49"
        android:textSize="12sp"
        android:textColor="#A09F9F"
        android:layout_below="@+id/print_info1"
        android:layout_marginTop="18dp"
        android:layout_toEndOf="@+id/duetime1"
        android:layout_marginLeft="5dp"
 />

// 수령확인절차


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/check1"
        android:src="@drawable/code_unclicked"
        android:background="@null"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/bg_receipt"
        android:layout_marginBottom="17dp"
        android:layout_marginLeft="12dp"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/check2"
        android:src="@drawable/code_unclicked"
        android:background="@null"
        android:layout_marginRight="16.9dp"
        android:layout_alignTop="@+id/check3"
        android:layout_toStartOf="@+id/check3" />


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/check3"
        android:src="@drawable/code_unclicked"
        android:background="@null"
        android:layout_marginRight="16.9dp"
        android:layout_alignTop="@+id/check4"
        android:layout_toStartOf="@+id/check4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/check4"
        android:src="@drawable/code_unclicked"
        android:background="@null"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/bg_receipt"
        android:layout_marginBottom="17dp"
        android:layout_marginRight="12dp"/>



</RelativeLayout>
